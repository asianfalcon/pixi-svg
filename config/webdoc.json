{
  "templates": {
    "applicationName": "pixi-svg",
    "cleverLinks": false,
    "monospaceLinks": false,
    "footer": "Made with â™¥ by Matt Karl (github.com/bigtimebuddy)",
    "copyright": "PIXI Copyright &copy; 2022 Matt Karl.",
    "meta": {
      "title": "PixiJS SVG Graphics Plugin",
      "description": "SVG to Graphics DisplayObject for PIXI",
      "keyword": "PixiJS, Rendering, Graphics, SVG, Vector"
    },
    "linenums": true,
    "default": {
      "outputSourceFiles": true
    }
  },
  "template": {
    "readme": "./README.md",
    "repository": "https://github.com/bigtimebuddy/pixi-svg/"
  },
  "tags": {
    "allowUnknownTags": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false
  },
  "source": {
    "include": [
      "src/**/*.ts"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "private": false,
    "lenient": false,
    "destination": "./docs",
    "template": "@pixi/webdoc-template"
  }
}